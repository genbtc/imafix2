#!/bin/bash
gcc -march=znver3 -mshstk -DHAVE_CONFIG_H -I. -I.. -include config.h  -g -O2 -Wall -Wstrict-prototypes -pipe -MT imafix2.d -MD -MP -c imafix2.c -o imafix2.o
gcc -march=znver3 -mshstk -Wno-deprecated-declarations -DHAVE_CONFIG_H -I. -I.. -include config.h -g -O2 -Wall -Wstrict-prototypes -c libimaevm.c -o libimaevm.o
gcc -march=znver3 -mshstk -g -O2 -Wall -pipe imafix2.o -lcrypto -lkeyutils libimaevm.o -o imafix2
strip --strip-unneeded imafix2
echo "Completed compiling. imafix2"
